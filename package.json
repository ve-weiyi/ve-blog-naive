{
  "name": "ve-blog-naive",
  "version": "1.0.0",
  "description": "分享个人学习记录的个人博客",
  "keywords": [
    "个人博客",
    "前端",
    "全栈",
    "vue3",
    "vite",
    "naive-ui"
  ],
  "author": "791422171@qq.com",
  "license": "MIT",
  "repository": "https://github.com/ve-weiyi/ve-blog-naive.git",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --host",
    "test": "vite --mode test --host",
    "prod": "vite --mode prod --host",
    "build": "vue-tsc --noEmit && vite --mode prod build",
    "build:dev": "vue-tsc --noEmit && vite --mode dev build",
    "build:test": "vue-tsc --noEmit && vite --mode test build",
    "preview": "vite preview --host",
    "format": "prettier --write src",
    "mirror:get": "pnpm config get registry",
    "mirror:set": "pnpm config set registry https://registry.pnpmmirror.com",
    "mirror:rm": "pnpm config rm registry",
    "backup": "git add . && git commit -m \"chore: backup\" && git push",
    "up": "npx pnpm-check-updates -u",
    "prepare": "",
    "depcheck": "depcheck",
    "lint-fix": "eslint --fix --ext .js,.ts,.jsx,.tsx,.vue src",
    "lint-prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "pnpm run lint-prettier && pnpm run lint-fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "@stomp/stompjs": "^7.1.1",
    "@vueuse/core": "^12.8.2",
    "aplayer": "^1.10.1",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "easy-typer-js": "^2.1.0",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.3.0",
    "prismjs": "^1.30.0",
    "swiper": "^11.2.6",
    "tocbot": "^4.36.3",
    "v-viewer": "^3.0.21",
    "viewerjs": "^1.11.7",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-html": "^3.2.2",
    "vue": "^3.5.14",
    "vue-cropper": "^1.1.4",
    "vue-masonry": "^0.16.0",
    "vue-router": "^4.5.1",
    "vue3-danmaku": "^1.6.1",
    "vue3-lazy": "1.0.0-alpha.1",
    "vue3-social-share": "^0.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.100",
    "@types/nprogress": "^0.2.3",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.1.0",
    "naive-ui": "^2.41.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.0",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^19.2.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-prismjs": "0.0.8",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@9.1.1",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
