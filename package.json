{
  "name": "ve-blog-naive",
  "version": "4.0.0",
  "description": "分享个人学习记录的个人博客",
  "keywords": [
    "个人博客",
    "前端",
    "全栈",
    "vue3",
    "vite",
    "typescript",
    "naive-ui"
  ],
  "author": "791422171@qq.com",
  "license": "MIT",
  "repository": "https://github.com/ve-weiyi/ve-blog-naive.git",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode development --host",
    "prod": "vite --mode production --host",
    "build": "vue-tsc --noEmit && vite --mode production build",
    "build:dev": "vue-tsc --noEmit && vite --mode development build",
    "preview": "vite preview --host",
    "format": "prettier --write src",
    "mirror:get": "pnpm config get registry",
    "mirror:set": "pnpm config set registry https://registry.pnpmmirror.com",
    "mirror:rm": "pnpm config rm registry",
    "backup": "git add . && git commit -m \"chore: backup\" && git push",
    "up": "npx pnpm-check-updates -u",
    "prepare": "",
    "depcheck": "pnpm exec depcheck",
    "lint-fix": "eslint --fix --ext .js,.ts,.jsx,.tsx,.vue src",
    "lint-prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "pnpm run lint-prettier && pnpm run lint-fix"
  },
  "dependencies": {
    "@kangc/v-md-editor": "^2.3.18",
    "@stomp/stompjs": "^7.2.1",
    "@vueuse/core": "^13.9.0",
    "aplayer": "^1.10.1",
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "easy-typer-js": "^2.1.0",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.22",
    "naive-ui": "^2.43.2",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "prismjs": "^1.30.0",
    "swiper": "^11.2.10",
    "tocbot": "^4.36.4",
    "v-viewer": "^3.0.22",
    "viewerjs": "^1.11.7",
    "vue": "^3.5.26",
    "vue-cropper": "^1.1.4",
    "vue-masonry": "^0.16.0",
    "vue-router": "^4.6.4",
    "vue3-danmaku": "^1.6.6",
    "vue3-lazy": "1.0.0-alpha.1",
    "vue3-social-share": "^0.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.19.3",
    "@types/nprogress": "^0.2.3",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "sass": "^1.97.1",
    "typescript": "^5.9.3",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.4.1",
    "vite-plugin-banner": "^0.8.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-prismjs": "^0.0.11",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.7.9",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=18.19.100",
    "pnpm": ">=8.15.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
